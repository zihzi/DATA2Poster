{
  "schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/zihzi/DATA2Poster/refs/heads/main/data/movies_record.csv"
  },
  "transform": [],
  "mark": "point",
  "encoding": {
    "x": {
      "field": "Domestic m",
      "type": "quantitative",
      "axis": {
        "title": "Domestic Gross (m)"
      },
      "scale": {
        "zero": false
      }
    },
    "y": {
      "field": "Worldwide m",
      "type": "quantitative",
      "axis": {
        "title": "Worldwide Gross ($m)"
      },
      "scale": {
        "zero": false
      }
    },
    "color": {
      "value": "yellowgreen"
    }
  }
}